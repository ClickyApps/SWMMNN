<?xml version='1.0' encoding='utf-8'?>
<widget id="uk.org.swmmnn.app" version="1.2.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>SWMMNN</name>
    <description>
		Information about the Southern West Midlands Maternity and Newborn Network of hospitals. This app includes information for families of premature and sick babies, with key information about each hospital and unit available.
	</description>
    <author email="josh@clicky.co.uk" href="http://www.clicky.co.uk">
	  Clicky Media
	</author>
    <gap:config-file overwrite="true" parent="UISupportedInterfaceOrientations" platform="ios">
        <array>
            <string>UIInterfaceOrientationLandscapeOmg</string>
        </array>
    </gap:config-file>
    <gap:plugin name="org.apache.cordova.inappbrowser" version="0.3.3" />
    <gap:plugin name="com.phonegap.plugin.statusbar" version="1.1.0" />
    <gap:config-file parent="UIStatusBarHidden" platform="ios">
        <true />
    </gap:config-file>
    <gap:config-file parent="UIViewControllerBasedStatusBarAppearance" platform="ios">
        <false />
    </gap:config-file>
    <access origin="*" />
    <gap:splash src="splash.png" />
    <gap:splash height="480" src="splash/ios/Default.png" width="320" />
    <gap:splash height="960" src="splash/ios/Default_at_2x.png" width="640" />
    <gap:splash height="1136" src="splash/ios/Default_iphone5.png" width="640" />
    <gap:splash height="768" src="splash/ios/Default-Landscape.png" width="1024" />
    <gap:splash height="1024" src="splash/ios/Default-Portrait.png" width="768" />
    <icon gap:platform="ios" height="57" src="Icon.png" width="57" />
    <icon gap:platform="ios" height="72" src="Icon-72.png" width="72" />
    <icon gap:platform="ios" height="114" src="Icon@2x.png" width="114" />
    <icon gap:platform="ios" height="144" src="Icon-72@2x.png" width="144" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="disable-cursor" value="true" />
    <preference name="android-installLocation" value="auto" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="permissions" value="none" />
</widget>
